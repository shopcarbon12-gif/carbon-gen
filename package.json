{
  "name": "carbon-gen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "dev": "powershell -ExecutionPolicy Bypass -File scripts/dev-local.ps1",
    "dev:raw": "next dev",
    "dev:3000": "next dev -p 3000",
    "start": "next start",
    "lint": "eslint",
    "hash-password": "node scripts/hash-password.mjs",
    "e2e": "node scripts/run-e2e.mjs",
    "check:upstash": "node scripts/check-upstash.mjs",
    "check:sync": "node scripts/check-sync.mjs",
    "check:shopify": "node scripts/check-shopify-smoke.mjs",
    "start:tunnel": "powershell -ExecutionPolicy Bypass -File scripts/start-tunnel.ps1",
    "start:local": "powershell -ExecutionPolicy Bypass -File scripts/start-local-stack.ps1",
    "stop:local": "powershell -ExecutionPolicy Bypass -File scripts/stop-local-stack.ps1",
    "backup:restore": "powershell -ExecutionPolicy Bypass -File scripts/create-restore.ps1",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.16.4",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/auth-helpers-react": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "bcryptjs": "^3.0.3",
    "next": "16.1.6",
    "openai": "^6.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.65.0"
  }
}
